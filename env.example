# Server Configuration
PORT=3008
NODE_ENV=development

# Solana Configuration
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
SOLANA_PRIVATE_KEY=your_wallet_private_key_here
SOLANA_PUBLIC_KEY=your_wallet_public_key_here

# Volume Bot Configuration
VOLUME_TRACKING_ENABLED=true
UPDATE_INTERVAL=5000
MIN_VOLUME_THRESHOLD=1000
TRACK_MULTIPLE_TOKENS=true

# Raydium Specific Configuration
RAYDIUM_PROGRAM_ID=675kPX9MHTjS2zt1qfr1NYHuzeLXfQM9H24wFSUt1Mp8
RAYDIUM_API_URL=https://api.raydium.io

# Volume Tracking Settings
TRACK_24H_VOLUME=true
TRACK_1H_VOLUME=true
TRACK_5M_VOLUME=true
VOLUME_ALERT_THRESHOLD=10000
VOLUME_SPIKE_MULTIPLIER=2.0

# Organic Strategy Configuration
ORGANIC_STRATEGY_ENABLED=true
MIN_UNIQUE_WALLETS=10
MAX_WALLET_CONCENTRATION=0.3
MIN_TRANSACTION_COUNT=5
WASH_TRADING_DETECTION=true
PATTERN_ANALYSIS_ENABLED=true

# Logging
LOG_LEVEL=info
LOG_FILE=logs/volume.log

